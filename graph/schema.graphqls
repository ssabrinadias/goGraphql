type List {
  id: ID!
  title: String!
  userId: ID!
  items: [String]!
}

type User {
  id: ID!
  name: String!
  city: String
}

type Query {
  list: [List!]!
  user: [User!]!
}

input NewList {
  title: String!
  userId: ID!
  items: [String]!
}

input NewUser {
  name: String!
  city: String
}

type Mutation {
  createList(input: NewList!): List!
  createUser(input: NewUser!): User!
}